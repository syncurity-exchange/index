{
    "api_key": {
        "default": "{{ st2kv.system.irflow_api_key }}",
        "description": "IR-Flow API Key",
        "required": false,
        "secret": true,
        "type": "string"
    },
    "api_user": {
        "default": "{{ st2kv.system.irflow_api_user }}",
        "description": "IR-Flow API User",
        "required": false,
        "type": "string"
    },
    "protocol": {
        "default": "https",
        "description": "Configure http or https - defaults to https",
        "enum": [
            "http",
            "https"
        ],
        "required": false,
        "type": "string"
    },
    "server": {
        "default": "{{ st2kv.system.irflow_server}}",
        "description": "IR-Flow server address",
        "required": false,
        "type": "string"
    },
    "wf_key": {
        "default": "{{ st2kv.system.pan_wildfire_key }}",
        "description": "PAN WildFire API Key",
        "required": true,
        "secret": true,
        "type": "string"
    }
}
