{
    "class_name": "SophosEventSensor",
    "description": "Get Sophos Events",
    "entry_point": "sophos_get_events.py",
    "trigger_types": [
        {
            "description": "An Event from Sophos",
            "name": "event",
            "payload_schema": {
                "properties": {
                    "device": {
                        "type": "string"
                    },
                    "eventId": {
                        "type": "string"
                    },
                    "eventName": {
                        "type": "string"
                    },
                    "eventTime": {
                        "type": "string"
                    },
                    "eventType": {
                        "type": "string"
                    },
                    "severity": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    ]
}
