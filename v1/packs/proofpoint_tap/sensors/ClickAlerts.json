{
    "class_name": "ClickAlerts",
    "description": "Get clicks from ProofPoint",
    "enabled": false,
    "entry_point": "click_alerts.py",
    "poll_interval": 30,
    "trigger_types": [
        {
            "description": "Click Alert from ProofPoint",
            "name": "click",
            "payload_schema": {
                "properties": {
                    "response": {
                        "type": "object"
                    }
                },
                "type": "object"
            }
        },
        {
            "description": "ProofPoint Click Alert sensor failure",
            "name": "click.fail",
            "payload_schema": {
                "properties": {
                    "response": {
                        "type": "object"
                    }
                },
                "type": "object"
            }
        }
    ]
}
