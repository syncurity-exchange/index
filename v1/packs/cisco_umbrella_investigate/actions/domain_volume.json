{
    "description": "Number of DNS queries made per hour to the specified domain by users",
    "enabled": true,
    "entry_point": "run.py",
    "name": "domain_volume",
    "parameters": {
        "domain": {
            "position": 0,
            "required": true,
            "type": "string"
        },
        "match": {
            "default": "all",
            "position": 3,
            "required": false,
            "type": "string"
        },
        "method": {
            "default": "domain_volume",
            "description": "the name of the method in pyinvestigate",
            "immutable": true,
            "type": "string"
        },
        "start": {
            "position": 1,
            "required": false,
            "type": "string"
        },
        "stop": {
            "position": 2,
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
