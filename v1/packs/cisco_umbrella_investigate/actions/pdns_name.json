{
    "description": "Returns data from DNS queries that resolvers received, and categorization data.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "pdns_name",
    "parameters": {
        "limit": {
            "position": 1,
            "required": false,
            "type": "string"
        },
        "method": {
            "default": "pdns_name",
            "description": "the name of the method in pyinvestigate",
            "immutable": true,
            "type": "string"
        },
        "name": {
            "position": 0,
            "required": true,
            "type": "string"
        },
        "offset": {
            "default": 0,
            "position": 2,
            "required": false,
            "type": "integer"
        },
        "recordType": {
            "position": 5,
            "required": false,
            "type": "string"
        },
        "sortby": {
            "position": 4,
            "required": false,
            "type": "string"
        },
        "sortorder": {
            "default": "desc",
            "position": 3,
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
