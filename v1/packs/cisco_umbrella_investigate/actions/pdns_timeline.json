{
    "description": "Get a snapshot of passive DNS and Umbrella categorization history for a domain name.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "pdns_timeline",
    "parameters": {
        "method": {
            "default": "pdns_timeline",
            "description": "the name of the method in pyinvestigate",
            "immutable": true,
            "type": "string"
        },
        "recordType": {
            "position": 1,
            "required": false,
            "type": "string"
        },
        "timeline": {
            "position": 0,
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
