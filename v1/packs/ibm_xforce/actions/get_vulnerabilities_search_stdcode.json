{
    "description": "Returns the vulnerability associated with the entered stdcode.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "get_vulnerabilities_search_stdcode",
    "parameters": {
        "body_args": {
            "default": "[]",
            "description": "the body argument names",
            "immutable": true,
            "type": "string"
        },
        "endpoint": {
            "default": "/vulnerabilities/search/{stdcode}",
            "description": "the endpoint to use",
            "immutable": true,
            "type": "string"
        },
        "method": {
            "default": "GET",
            "description": "the HTTP method to use",
            "immutable": true,
            "type": "string"
        },
        "stdcode": {
            "description": "Search term, For example, CVE-2014-2601, BID-69453, US-CERT VU#546340,\nRHSA20131456",
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
