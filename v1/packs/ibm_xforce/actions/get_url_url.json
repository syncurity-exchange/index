{
    "description": "Returns the URL report for the entered URL.",
    "enabled": true,
    "entry_point": "run.py",
    "name": "get_url_url",
    "parameters": {
        "body_args": {
            "default": "[]",
            "description": "the body argument names",
            "immutable": true,
            "type": "string"
        },
        "endpoint": {
            "default": "/url/{url}",
            "description": "the endpoint to use",
            "immutable": true,
            "type": "string"
        },
        "method": {
            "default": "GET",
            "description": "the HTTP method to use",
            "immutable": true,
            "type": "string"
        },
        "url": {
            "description": "url For example, ibm.com, www.ibm.com/smarterplanet",
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
