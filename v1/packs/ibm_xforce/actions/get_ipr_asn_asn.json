{
    "description": "Returns all networks that are assigned to an Autonomous System Number",
    "enabled": true,
    "entry_point": "run.py",
    "name": "get_ipr_asn_asn",
    "parameters": {
        "asn": {
            "description": "A valid ASN number\n```\nASN1\nASN5387\n```",
            "required": true,
            "type": "string"
        },
        "body_args": {
            "default": "[]",
            "description": "the body argument names",
            "immutable": true,
            "type": "string"
        },
        "endpoint": {
            "default": "/ipr/asn/{asn}",
            "description": "the endpoint to use",
            "immutable": true,
            "type": "string"
        },
        "method": {
            "default": "GET",
            "description": "the HTTP method to use",
            "immutable": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
