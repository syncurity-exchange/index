{
    "description": "Determine Email Type",
    "enabled": true,
    "entry_point": "determine_email_type.py",
    "name": "determine_email_type",
    "parameters": {
        "alert_num": {
            "description": "IR-Flow Alert Number which contains phishing attachment",
            "required": true,
            "type": "string"
        },
        "exclude_image_extensions": {
            "default": "true",
            "description": "Excludes images from analysis",
            "required": false,
            "type": "string"
        },
        "internal_domains": {
            "description": "A comma and space separated list of internal domains",
            "required": false,
            "type": "string"
        },
        "irflow_api_key": {
            "default": "{{ st2kv.system.irflow_api_key | decrypt_kv }}",
            "description": "IR-Flow API Key",
            "required": true,
            "secret": true,
            "type": "string"
        },
        "irflow_api_user": {
            "default": "{{ st2kv.system.irflow_api_user}}",
            "description": "IR-Flow API User",
            "required": true,
            "type": "string"
        },
        "irflow_server": {
            "default": "{{ st2kv.system.irflow_server}}",
            "description": "IR-Flow Server Address",
            "required": true,
            "type": "string"
        },
        "whitelist": {
            "description": "A comma and space separated list of domains to whitelist",
            "required": false,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
