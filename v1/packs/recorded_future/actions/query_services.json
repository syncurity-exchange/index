{
    "description": "Query Recorded Future for a particular indicator",
    "enabled": true,
    "entry_point": "query_services.py",
    "name": "query_services",
    "parameters": {
        "indicator": {
            "description": "Indicator to lookup",
            "required": true,
            "type": "string"
        },
        "ioc_type": {
            "description": "Type of indicator",
            "enum": [
                "ip",
                "hash",
                "malware",
                "url",
                "domain"
            ],
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
