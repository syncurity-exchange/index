{
    "description": "Get Count of Infected Clients by ReportType: Hour,Day,Week,Month",
    "enabled": true,
    "entry_point": "run.py",
    "name": "count_infected_clients",
    "parameters": {
        "ReportType": {
            "description": "the type of report to use",
            "enum": [
                "Hour",
                "Day",
                "Week",
                "Month"
            ],
            "required": true,
            "type": "string"
        },
        "args": {
            "default": "['ReportType', 'oldTime', 'timestamp']",
            "description": "the argument names",
            "immutable": true,
            "type": "string"
        },
        "body": {
            "default": "",
            "description": "the request body to use",
            "immutable": true,
            "type": "string"
        },
        "body_args": {
            "default": "[]",
            "description": "the body argument names",
            "immutable": true,
            "type": "string"
        },
        "endpoint": {
            "default": "/stats/client/infection/{}/{}/to/{}",
            "description": "the endpoint to use",
            "immutable": true,
            "type": "string"
        },
        "header": {
            "default": "{'Content-Type': 'application/json'}",
            "description": "the header to use",
            "immutable": true,
            "type": "string"
        },
        "method": {
            "default": "GET",
            "description": "the HTTP method to use",
            "immutable": true,
            "type": "string"
        },
        "oldTime": {
            "description": "the starting timestamp",
            "required": true,
            "type": "string"
        },
        "timestamp": {
            "description": "the ending timestamp",
            "required": true,
            "type": "string"
        }
    },
    "runner_type": "python-script"
}
