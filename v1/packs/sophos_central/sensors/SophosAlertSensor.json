{
    "class_name": "SophosAlertSensor",
    "description": "Get Sophos Alerts",
    "enabled": false,
    "entry_point": "get_alerts.py",
    "trigger_types": [
        {
            "description": "An Alert from Sophos",
            "name": "alert",
            "payload_schema": {
                "properties": {
                    "alertId": {
                        "type": "string"
                    },
                    "alertType": {
                        "type": "string"
                    },
                    "creationDate": {
                        "type": "string"
                    },
                    "device": {
                        "type": "string"
                    },
                    "severity": {
                        "type": "string"
                    },
                    "summary": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    ]
}
