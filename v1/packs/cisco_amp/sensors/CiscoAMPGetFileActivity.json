{
    "class_name": "CiscoAMPGetFileActivity",
    "description": "Get File Activity Service",
    "enabled": false,
    "entry_point": "get_file_activity.py",
    "poll_interval": 30,
    "trigger_types": [
        {
            "description": "File Activity from Cisco AMP",
            "name": "file_activity",
            "payload_schema": {
                "properties": {
                    "response": {
                        "type": "object"
                    }
                },
                "type": "object"
            }
        },
        {
            "description": "Cisco AMP sensor failure",
            "name": "fail",
            "payload_schema": {
                "type": "object"
            }
        }
    ]
}
